<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="319.0" prefWidth="338.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.Controllers.SignInController">
   <children>
      <Label layoutX="112.0" layoutY="47.0" text="Sign in!" />
      <Label layoutX="14.0" layoutY="99.0" text="Enter your email address" />
      <TextField fx:id="emailTextField" layoutX="169.0" layoutY="95.0" />
      <TextField fx:id="passwordTextField" layoutX="169.0" layoutY="152.0" />
      <Label layoutX="14.0" layoutY="156.0" text="Enter your password:" />
      <Button fx:id="signInButton" layoutX="133.0" layoutY="199.0" mnemonicParsing="false" onAction="#SignInButton" text="Sign In" />
      <Label layoutX="182.0" layoutY="249.0" text="Don't have an account?" />
      <Button fx:id="creatingAccountButton" layoutX="194.0" layoutY="267.0" mnemonicParsing="false" onAction="#createAccountButton" text="Create an account" />
   </children>
</AnchorPane>
