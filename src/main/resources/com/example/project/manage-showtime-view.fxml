<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="518.0" prefWidth="522.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.Controllers.ManageShowTimeViewController">
   <children>
      <Label layoutX="148.0" layoutY="34.0" text="Manage ShowTimes">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="showTimeTableView" layoutX="41.0" layoutY="126.0" prefHeight="295.0" prefWidth="299.0">
        <columns>
          <TableColumn fx:id="movieColumn" prefWidth="114.0" text="Movie" />
          <TableColumn fx:id="dateColumn" prefWidth="102.0" text="Date" />
            <TableColumn fx:id="timeColumn" prefWidth="82.0" text="Time" />
        </columns>
      </TableView>

      <Button fx:id="addButton" layoutX="374.0" layoutY="156.0" mnemonicParsing="false" onAction="#handleAddButton" prefHeight="42.0" prefWidth="88.0" text="     Add &#10;ShowTime" />
      <Button fx:id="deleteShowTimeButton" layoutX="374.0" layoutY="379.0" mnemonicParsing="false" onAction="#handleDeleteButton" prefHeight="42.0" prefWidth="88.0" text="   Delete&#10;ShowTime" />
      <Button fx:id="consultShowTimeButton" layoutX="374.0" layoutY="312.0" mnemonicParsing="false" onAction="#handleConsultButton" prefHeight="42.0" prefWidth="88.0" text="   Consult&#10; ShowTime" />
      <Button fx:id="backButton" layoutX="209.0" layoutY="454.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="25.0" prefWidth="88.0" text="Back" />
      <Button fx:id="modifyButton" layoutX="374.0" layoutY="238.0" mnemonicParsing="false" onAction="#handleModifyButton" prefHeight="42.0" prefWidth="88.0" text="   Modify&#10;ShowTime" />

   </children>
</AnchorPane>
